<div style="width: 910px; margin:auto;">
	<div class="create-project-sidebar-container">
	</div>
	<div class="form-container">
		<form class="form create-project-form">
			<div>
				<label for="name">Project Name</label>
				<input type="text" name="name" required/>
			</div>
			
			<div>
				<label for="description">Project Description</label>
				<textarea name="description" rows="6"></textarea>
			</div>
			
			<div>
				<label for="github_url">Github URL (If any)</label>
				<input type="text" name="github_url" />
			</div>
			
			<div>
				<label for="website">Website (If any)</label>
				<input type="text" name="website" />
			</div>
			<div>
				<input class="create-project-submit" type="button" value="Submit" />
			</div>
		</form>
	</div>
</div>
